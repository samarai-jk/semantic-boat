# ------------------------------------------------
# Flash (OpenOCD)
# Override OPENOCD and OPENOCD_SCRIPTS as needed, or set via environment
OPENOCD ?= E:/Tools/xpack-openocd-0.12.0-7/bin/openocd.exe
OPENOCD_SCRIPTS ?= E:/Tools/xpack-openocd-0.12.0-7/scripts
OPENOCD_IF ?= interface/stlink.cfg
OPENOCD_TARGET ?= target/stm32f3x.cfg

.PHONY: flash
flash: $(BUILD_DIR)/$(TARGET).elf
	"$(OPENOCD)" -s "$(OPENOCD_SCRIPTS)" -f $(OPENOCD_IF) -f $(OPENOCD_TARGET) -c "program $< verify reset exit"


